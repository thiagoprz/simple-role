{
  "name": "thiagoprz/simple-role",
  "description": "Laravel package to add a simple role based authentication system to any project",
  "type": "library",
  "require": {
    "php": "^7.4 || ^8.0 || ^8.1",
    "laravel/framework": "^5.5||^7.0||^8.6||^9.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Thiago Przyczynski",
      "email": "przyczynski@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "Thiagoprz\\SimpleRole\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Thiagoprz\\SimpleRole\\SimpleRoleServiceProvider"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
